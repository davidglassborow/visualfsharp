# Sanity check - simply check that the option is valid
	SOURCE=dummy.fs SCFLAGS="--deterministic" 
	SOURCE=dummy.fs SCFLAGS="--deterministic+" 
	SOURCE=dummy.fs SCFLAGS="--deterministic-" 
	SOURCE=dummy.fs SCFLAGS="--deterministic"   FSIMODE=EXEC COMPILE_ONLY=1
	SOURCE=dummy.fs SCFLAGS="--deterministic+"  FSIMODE=EXEC COMPILE_ONLY=1
	SOURCE=dummy.fs SCFLAGS="--deterministic-"  FSIMODE=EXEC COMPILE_ONLY=1

# Confirm wildcard versions are not allowed
	SOURCE=wildcardVersionSpecifed.fs SCFLAGS="--deterministic"

# Confirm specific versions are allowed
	SOURCE=specificVersionSpecifed.fs SCFLAGS="--deterministic"

# Confirm in normal not-determinstic mode, the same file compiled twice leads to different exes & pdbs
# NOTE: we need to use the timeout because non-determinism in fsc is due to datetime, so we need to guarentee at least 1 sec difference
	SOURCE=dummy.fs                            PRECMD="\$FSC_PIPE dummy.fs && copy dummy.* dummy2.* && timeout 1" POSTCMD="\$FSI_PIPE --nologo --quiet --exec binaryCompare.fsx dummy dummy2 false"
	SOURCE=dummy.fs SCFLAGS="--debug:full"     PRECMD="\$FSC_PIPE --debug:full dummy.fs && copy dummy.* dummy2.* && timeout 1" POSTCMD="\$FSI_PIPE --nologo --quiet --exec binaryCompare.fsx dummy dummy2 false"
	SOURCE=dummy.fs SCFLAGS="--debug:pdbonly"  PRECMD="\$FSC_PIPE --debug:pdbonly dummy.fs && copy dummy.* dummy2.* && timeout 1" POSTCMD="\$FSI_PIPE --nologo --quiet --exec binaryCompare.fsx dummy dummy2 false"
	SOURCE=dummy.fs SCFLAGS="--debug:portable" PRECMD="\$FSC_PIPE --debug:portable dummy.fs && copy dummy.* dummy2.* && timeout 1" POSTCMD="\$FSI_PIPE --nologo --quiet --exec binaryCompare.fsx dummy dummy2 false"
	SOURCE=dummy.fs SCFLAGS="--debug:embedded" PRECMD="\$FSC_PIPE --debug:embedded dummy.fs && copy dummy.* dummy2.* && timeout 1" POSTCMD="\$FSI_PIPE --nologo --quiet --exec binaryCompare.fsx dummy dummy2 false"

